@using pax.schafkopf.Shared
@using pax.schafkopf.lib.Enums
<div>
    @SKData.Version.ToString()
</div>
@if (table.isLoading)
{
    <button class="btn btn-sm btn-primary" disabled>
        <span class="spinner-border spinner-border-sm"></span>
        .
    </button>
}
else
{
    <button class="btn btn-primary ml-3" disabled>
        .
    </button>
}
@if (!String.IsNullOrEmpty(table.Error))
{
    <div>
        @table.Error
    </div>
}
@if (table.Table != null)
{

}
<span class="oi oi-circle-check text-success"></span>
